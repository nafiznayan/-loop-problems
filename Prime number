#include<stdio.h>
#include<math.h>
int main()
{
    int n,i,c=0;
    printf("Enter a number:");
    scanf("%d",&n);
    int m=sqrt(n);
    for(i=2;i<=m;i++)
    {
        if(n%i==0)
        {
            c++;
            break;
        }
    }
    if(c==0) printf("%d is a prime number.",n);
    else printf("%d is not a prime number.",n);
}
