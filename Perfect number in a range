#include<stdio.h>
int main()
{
    int n,i,sum;
    int mn,mx;
    printf("mn=");
    scanf("%d",&mn);
    printf("mx=");
    scanf("%d",&mx);
    printf("perfect number in that range:");
    for(n=mn;n<=mx;n++)
    {
        sum=0;
        for(i=1;i<n;i++)
        {
            if(n%i==0)
            {
                sum+=i;
            }
        }
        if(sum==n)
        {
            printf("%d "n);
        }
    }
}
